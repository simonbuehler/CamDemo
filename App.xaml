<chromiumFxInfra:ChromiumFxWebBrowserApp x:Class="CamDemo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CamDemo"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:chromiumFxInfra="clr-namespace:Neutronium.WebBrowserEngine.ChromiumFx;assembly=Neutronium.WebBrowserEngine.ChromiumFx"
                                         xmlns:vm1="clr-namespace:CamDemo.ViewModel"
                                          xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d1p1:Ignorable="d"
                                         StartupUri="MainWindow.xaml">
    <chromiumFxInfra:ChromiumFxWebBrowserApp.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <vm1:ViewModelLocator
            
                x:Key="Locator"
                d:IsDataSource="True" />

        </ResourceDictionary>

    </chromiumFxInfra:ChromiumFxWebBrowserApp.Resources>
</chromiumFxInfra:ChromiumFxWebBrowserApp>
